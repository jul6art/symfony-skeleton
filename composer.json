{
    "type": "project",
    "name": "jul6art/symfony-skeleton",
    "description": "Symfony Back-office package",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "damienharper/doctrine-audit-bundle": "^3.4",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "2.6.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "fzaninotto/faker": "^1.8",
        "gedmo/doctrine-extensions": "^2.4",
        "google/recaptcha": "^1.2",
        "knplabs/knp-menu-bundle": "^2.0",
        "lcobucci/jwt": "^3.3",
        "lexik/translation-bundle": "^4.0",
        "saacsos/randomgenerator": "^1.1",
        "sensio/framework-extra-bundle": "^5.3",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^5.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lock": "^5.0",
        "symfony/mercure-bundle": "^0.2.3",
        "symfony/messenger": "4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.1",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^5.0",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5",
        "willdurand/js-translation-bundle": "^2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "friendsoftwig/twigcs": "^3.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "sebastian/phpcpd": "^4.1",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "jaschilz/php-coverage-badger": "^2.0",
        "phpro/grumphp": "^0.18.0",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
