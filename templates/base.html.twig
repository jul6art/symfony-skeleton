<head>
    {% block head %}
        <title>
            {% block title %}{{ setting_value(constant('App\\Entity\\Constants\\SettingName::SETTING_NAME_BASE_TITLE'), constant('App\\Entity\\Constants\\SettingName::SETTING_VALUE_BASE_TITLE')) }}{% endblock title %}
        </title>
        <meta charset="utf-8"/>
        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
        <meta name="description" content="symfony skeleton by Jul6art"/>
        <meta name="author" content="dev in the hood"/>

        {% block stylesheets %}{% endblock stylesheets %}

    {% endblock head %}
</head>
<body class="{% block body_class %}{% endblock body_class %}">
{% block body_content %}{% endblock body_content %}
{% block javascripts %}{% endblock javascripts %}
</body>
</html>
